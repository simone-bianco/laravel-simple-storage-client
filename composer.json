{
    "name": "dolphin/simple-storage-client",
    "description": "Laravel client for Dolphin Simple Storage Server - A plug-and-play package for storing and retrieving ZIP files",
    "keywords": [
        "laravel",
        "storage",
        "client",
        "api",
        "dolphin",
        "zip"
    ],
    "homepage": "https://github.com/dolphin/simple-storage-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Dolphin",
            "email": "info@dolphin.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Dolphin\\SimpleStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dolphin\\SimpleStorage\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dolphin\\SimpleStorage\\SimpleStorageServiceProvider"
            ],
            "aliases": {
                "SimpleStorage": "Dolphin\\SimpleStorage\\Facades\\SimpleStorage"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
